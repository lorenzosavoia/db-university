domanda 1:SELECT * FROM `students` WHERE Year(`date_of_birth`) = 1990
domanda 2:SELECT * FROM `courses` WHERE cfu > 10
domanda 3:SELECT * FROM `students` WHERE YEAR(CURDATE()) - YEAR(date_of_birth) > 30
domanda 4:SELECT * FROM `courses` WHERE `year` = 1 && `period` LIKE "I semestre"
domanda 5:SELECT * FROM `exams` WHERE `date` = "2020/06/20" AND `hour` > "14:00:00"
domanda 6:SELECT * FROM `degrees` WHERE `name` LIKE "Corso di Laurea Magistrale%"
domanda 7:SELECT COUNT(id) FROM `departments`
domanda 8:SELECT COUNT(*) FROM `teachers` WHERE `phone` IS NULL
